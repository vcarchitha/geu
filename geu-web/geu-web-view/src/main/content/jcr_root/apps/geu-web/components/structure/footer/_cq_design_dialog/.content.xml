<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Footer"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"/>
        <items jcr:primaryType="nt:unstructured">
            <tab_logo
                jcr:primaryType="nt:unstructured"
                jcr:title="Logo"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <footerLogo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Footer Logo"
                                name="./footerLogo"
                                rootPath="/content/dam"/>
                            <footerURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Footer Logo URL"
                                name="./footerURL"
                                rootPath="/content"/>
                        </items>
                    </columns>
                </items>
            </tab_logo>
            <social_links
                jcr:primaryType="nt:unstructured"
                jcr:title="Social Media Links"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <fbpath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Facebook URL"
                                name="./facebookURL"/>
                            <fbcheck
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                defaultChecked="{Boolean}false"
                                name="./facebookCheck"
                                text="Hide Facebook Link"
                                uncheckedValue="false"
                                value="true"/>
                            <twitterPath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Twitter URL"
                                name="./twitterURL"/>
                            <twitterCheck
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                defaultChecked="{Boolean}false"
                                name="./twitterCheck"
                                text="Hide Twitter Link"
                                uncheckedValue="false"
                                value="true"/>
                            <linkedInPath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="LinkedIn URL"
                                name="./linkedInURL"/>
                            <linkedInCheck
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                defaultChecked="{Boolean}false"
                                name="./linkedInCheck"
                                text="Hide LinkedIn Link"
                                uncheckedValue="false"
                                value="true"/>
                            <youTubePath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="YouTube URL"
                                name="./youtubeURL"/>
                            <youTubeCheck
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                defaultChecked="{Boolean}false"
                                name="./youtubeCheck"
                                text="Hide YouTube Link"
                                uncheckedValue="false"
                                value="true"/>
                            <googlePlus
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Google Plus URL"
                                name="./googlePlusURL"/>
                            <googleCheck
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                defaultChecked="{Boolean}false"
                                name="./googleCheck"
                                text="Hide YouTube Link"
                                uncheckedValue="false"
                                value="true"/>
                            <instagramPath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Instagram URL"
                                name="./instagramURL"/>
                            <instagramCheck
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                defaultChecked="{Boolean}false"
                                name="./instagramCheck"
                                text="Hide Instagram Link"
                                uncheckedValue="false"
                                value="true"/>
                            <snapchatPath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Snapchat URL"
                                name="./snapchatURL"/>
                            <snapchatCheck
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                defaultChecked="{Boolean}false"
                                name="./snapchatCheck"
                                text="Hide Snapchat Link"
                                uncheckedValue="false"
                                value="true"/>
                        </items>
                    </columns>
                </items>
            </social_links>
            <tour_links
                jcr:primaryType="nt:unstructured"
                jcr:title="Tour Links"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <tourin
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Tour In Label"
                                name="./tourInLabel"/>
                            <virtualURL
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Virtual URL"
                                name="./virtualURL"/>
                            <virtualPath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="360 Image"
                                name="./virtualImage"
                                rootPath="/content"/>
                        </items>
                    </columns>
                </items>
            </tour_links>
            <footer_links
                jcr:primaryType="nt:unstructured"
                jcr:title="Footer Links"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <links
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="geu-web/dialog_widgets/generic_multifield">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                    name="./footerLinks">
                                    <layout
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <label
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="Label"
                                                    name="./label"/>
                                                <path
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                    fieldLabel="URL"
                                                    name="./linkPath"/>
                                                <newWindow
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                                    name="./newWindow"
                                                    text="new window"/>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </links>
                        </items>
                    </columns>
                </items>
            </footer_links>
            <copyright
                jcr:primaryType="nt:unstructured"
                jcr:title="Copyright"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <copyrightText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Copyright Text"
                                name="./copyrightText"/>
                        </items>
                    </columns>
                </items>
            </copyright>
        </items>
    </content>
</jcr:root>
